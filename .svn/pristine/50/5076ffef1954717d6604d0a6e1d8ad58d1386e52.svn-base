}
#shortcut .cw-icon {
    height: 31px;
    line-height: 28px;
    overflow: hidden;
    position: relative;
    z-index: 1;
    float: left;
    border: 1px solid #e3e4e5;
}
#shortcut .dorpdown-layer {
    top: 30px;
    display: none;
    position: absolute;
    border: 1px solid #ccc;
    background-color: #fff;
    -moz-box-shadow: 1px 2px 1px rgba(0, 0, 0, .1);
    box-shadow: 1px 2px 1px rgba(0, 0, 0, .1);
}
#shortcut .ci-right {
    display: none !important;
}
#shortcut .dorpdown {
    position: relative;
    z-index: 21
}
#shortcut .dorpdown:hover .dorpdown-layer, .hover .dorpdown-layer {
    display: block;
}
#shortcut .dorpdown:hover .cw-icon, .hover .cw-icon {
    padding-bottom: 2px;
    border-color: #ccc;
    border-bottom: none;
    background-color: #fff;
}
#shortcut .fr .cw-icon {
    padding-right: 20px !important;
    cursor: pointer;
}
#shortcut .fr .iconfont {
    position: absolute;
    right: 5px;
    top: 10px;
    width: 12px;
    height: 12px;
    line-height: 12px;
}
